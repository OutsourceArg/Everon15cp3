<odoo>
    <data noupdate="1">
        <!-- Acción planificada para conciliar pagos-->
        <record id="ir_cron_conciliar_pagos" model="ir.cron">
            <field name="name">Conciliación Automática de Pagos</field>
            <field name="user_id" ref="base.user_root"/>
			<field name="active">False</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field name="state">code</field>
            <field name="model_id" ref="payment.model_account_payment"/>
            <field name="code">model.reconciliar_pagos()</field>
        </record>
    </data>
</odoo>
